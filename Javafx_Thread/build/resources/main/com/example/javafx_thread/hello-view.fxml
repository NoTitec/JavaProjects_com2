<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.javafx_thread.HelloController" fx:id="anchorPane"
            prefHeight="400.0" prefWidth="600.0"
            >
<children>
    <Label fx:id="welcomeText" text="00:00:00" alignment="CENTER" layoutY="15.0" layoutX="25.0">
        <font>
            <Font size="20.0"></Font>
        </font>
    </Label>
    <Button fx:id="btnStart" text="시작" layoutX="46.0" layoutY="63.0" onAction="#btnStart"/>
    <Button fx:id="btnStop" text="멈춤" layoutX="110.0" layoutY="63.0" onAction="#btnStop"/>
</children>

</AnchorPane>
